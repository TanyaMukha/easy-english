{
  "expo": {
    "name": "easy-english",
    "slug": "easy-english",
    "scheme": "easy-english",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F23"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tanyamukha.easyenglish",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F23"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "903e251d-6c1c-4745-8c72-b9d8977c97f4"
      }
    },
    "assets": [
      "assets/images/*"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}